    <style>
        @media (max-width:767px) {
            body {
                margin-top: 2em !important;
            }
        }
        body {
            margin-top: 5em;
        }
        .sign-on-container .row {
            margin-top: 2em;
        }
        .sign-on-container .col-sm-6 {
            transition:all 0.3s ease;
        }
        .well {
            padding: 2em;
        }
        .fa {
            margin-right: 0.25em;
        }
        .row.toggle-registration-row, .row.toggle-sign-in-row {
            margin-top: 0em;
            text-align: center;
            margin-bottom: 5em;
        }
        .password-link {
            font-size: 0.85em;
            margin-top: 0.5em;
            width: 100%;
            display: block;
        }
        #showRegistration, #showSignIn {
            color: #407fbf;
        }
        #showRegistration:hover, #showSignIn:hover {
            color: #2d5986;
            text-decoration: underline;
            cursor: pointer;
        }
